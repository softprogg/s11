package model;

import java.util.*;


@Entity
public class Item implements Serializable{

@GeneratedValue(Generatedstrategy="Sequence")
@Id
int id;

@ManyToMany
@JoinTable(name="ORDER_S",  joinColumns=@JoinColumn(name="ITEM_ID"), inverseJoinColums=@JoinColumn(name="ORDER_ID"))
List <Order> orders;


}
