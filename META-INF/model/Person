package model;

import java.util.*;


@Entity
@Table(name="Person")
@Access(AccessType.FIELD)
public class Person implements Serializable{

@GeneratedValue(strategy = GenerationType.AUTO)
@Id
int id;

@Transient
int numOrd;

@Basic(fetch=FetchType.Lazy)
@Column(name="name")
String name;

String Surname;

@ManyToOne
@JoinColumn
Adress adress;

@Temporal(TemporalType.DATE)
Date dateLastAccess;

@ManyToMany
 @JoinTable( @JoinColumns=@JoinColumn(name="PERSON_ID")
            @inverseJoinColumn = @JoinColumn(name="ORDER_ID"))
  Map <Order> orders;
  
  


}
