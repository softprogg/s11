package model;

impoer java.util.*;


@Entity 
public class Order implements Serializable{

@GenerateValue(GeneratedStrategy="Sequence")
@Id
int id;

@ManyToMany
@JoinTable(name="ITEM_S", joinsColums = @JoinColumn(name="ORDER_ID"), inverseJoinColumns = @JoinColumn(name="ITEM_ID")   ) 
  List <Item> items;
  
  
 @ManyToOne 
 @JoinColumn(name="person")
 Person person;




}
